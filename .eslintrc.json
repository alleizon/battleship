{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "plugins": ["jest"],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["dist/main.js"],
    "rules": {
        "import/extensions": [
            "error", 
            {"js" : "always"}
        ]
    }
}
